                -ms-flex-order: 1;	
                order: 1;	
                @include flex(auto);
              }	
              span {	
                font-weight: 600;
                font-size: 20px;
                text-align: right;
                color: #3D4A51;
                display: block;	
              }	
              .item__property--label {	
