      @include flexbox();
      @include flex-wrap(wrap);

      .product-form__controls-group {
        margin: 0 1.5em 0 0;
        &:last-of-type {
          margin-right: 0;
          @media (max-width: 750px) {
            width: 100%;
          }
        }

        &.product-variant__ring-size {
          @include flex(1);
          @media (max-width:767){
            margin-left:10px;
          }
        }

        .product-form__item {
          padding: 0;
          margin-bottom: 1.5em;
          @include flexbox();
          @include flex-direction(column);
          flex-basis: 100%;

          label {
            font-size: 12px;
            letter-spacing: 2.4px;
            text-transform: uppercase;
            color: #FFF;
          }

          select {
            background-image: url('Down_Arrow.svg') !important;
            background-position: 92% center !important;
            background-repeat: no-repeat !important;
          }

          .product-form__input {
            outline: none;
            background: none;
            border: 1px solid #5D5D5D;
            min-height: auto;
            border-radius: 0;
            font-weight: 600;
            color: #FFF;
            @include flex(1);
          }
        }

        .product-quantity-box {
          max-width: 120px;
          @include flexbox();
          @include align-items(center);
          border: 1px solid #5D5D5D;

          .product-form__input--quantity {
            max-width: none;
            min-height: auto;
            border: none;
            text-align: center;
          }
          span {
            @include flexbox();
            cursor: pointer;

            &.icon-minus {
              padding: 11px 10px 11px 10px;
            }

            &.icon-plus {
              padding: 11px 10px 11px 10px;
            }

            svg {
              .minus-a,
              .plus-a {
                fill: #FFF;
                stroke: #FFF;
              }
            }
          }
        }

        .ring_size {
          padding: 5px 0;
          display: inline-block;
          @include align-self(center);
          border-bottom: 1px solid #FFF;
          text-transform: uppercase;
          letter-spacing: 1px;
          font-size: 10px;
          color: #FFF;
          @media (max-width: 768px) {
            margin-bottom: 1.5em;
          }

          svg {
            vertical-align: -7px;
            margin-right: 5px;
          }
        }
      }
    }

    .product-form__controls-group--submit {
