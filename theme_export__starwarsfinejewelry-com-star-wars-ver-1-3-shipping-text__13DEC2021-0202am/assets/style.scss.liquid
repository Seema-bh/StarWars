                  @include flex(1);
                }
                &.cart__remove {	
                  margin-top: 0;
                  @media (max-width: 768px) {
                    margin: 0 1em 0 0;
                  }
                  a {	
                    width: 26px;	
                    height: 26px;	
                    text-decoration: none;	
                    border: none;	
                    outline: none;	
                    display: block;	
                    font-size: 0;	
                    overflow: hidden;	
                    display: block;	

                    &:before {	
                      content: url('Remove-Delete_Icon.svg');	
                      display: inline-block;	
                      height: 26px;
                    }	

                    @media (max-width: 768px) {	
                      padding: 3px 5px;	
                      width: auto;	
                      height: auto;	
                      border: 1px solid #242424;
                      text-align: center;	
                      text-transform: uppercase;	
                      letter-spacing: 2px;	
                      font-size: 12px;	
                      line-height: 26px;	
                      color: #94A4AE;
                      @include flexbox();
                    }	
                  }	
                }	
                a.add_to_wishlist {	
                  padding: 0 10px;	
                  display: inline-block;	
                  text-transform: uppercase;
                  font-size: 12px;
                  letter-spacing: 1px;	
                  line-height: 26px;	
                  color: #94A4AE;

                  &:before {	
                    margin-right: 5px;
                    content: url('Wishlist_Button_Icon_-_Big.svg');	
                    display: inline-block;
                    vertical-align: -4px;
