.sw__banner {
  opacity: 0;
  visibility: hidden;
  transition: opacity 1s ease;
  -webkit-transition: opacity 1s ease;
  width: 100%;
  &.slick-initialized {
    visibility: visible;
    opacity: 1;

    img {
      vertical-align: bottom;
      width: 100%;
    }
  }

  .slick-slide {
    border: none;
  }

  .sw__banner-slide {
    position: relative;

    .sw__banner-img-cont {
      width: 50%;
      height: 100%;
      position: absolute;
      left: 0;
      top: 0;
      opacity: 0;
      // -webkit-transition: all 1500ms ease-in-out;
      // -moz-transition: all 1500ms ease-in-out;
      // -ms-transition: all 1500ms ease-in-out;
      // -o-transition: all 1500ms ease-in-out;
      // transition: all 1500ms ease-in-out;
      -webkit-transition: all 1s ease-in-out;
      -moz-transition: all 1s ease-in-out;
      -ms-transition: all 1s ease-in-out;
      -o-transition: all 1s ease-in-out;
      transition: all 1s ease-in-out;
      @media(max-width: 750px) {
        width: 100%;
        left: -5%;
        -webkit-transition: all 1.5s ease-in-out;
        -moz-transition: all 1.5s ease-in-out;
        -ms-transition: all 1.5s ease-in-out;
        -o-transition: all 1.5s ease-in-out;
        transition: all 1.5s ease-in-out;
      }
      img {
        height: 100%;
        object-fit: contain;
        @media(max-width: 750px) {
          height: auto;
        }
      }
    }

    .sw__banner-text {
      max-width: none;
      position: absolute;
      top: 0;
      right: 0;
      bottom: 85px;
      left: 0;
      @include flexbox();
      @media only screen and (max-width: 768px) {
        bottom: 0px;
      }

      &.content_center-aligned {
        @include justify-content(center);
      }

      &.content_right-aligned {
        @include justify-content(flex-end);
      }

      .sw__banner-text-container {
        padding: 80px 0;
        @include flex(0 0 40%);
        @include align-self(flex-start);
        position: relative;
        top: -50%;
        opacity: 0;
        // -webkit-transition: all 1500ms ease-in-out;
        // -moz-transition: all 1500ms ease-in-out;
        // -ms-transition: all 1500ms ease-in-out;
        // -o-transition: all 1500ms ease-in-out;
        // transition: all 1500ms ease-in-out;
        -webkit-transition: all 1s ease-in-out;
        -moz-transition: all 1s ease-in-out;
        -ms-transition: all 1s ease-in-out;
        -o-transition: all 1s ease-in-out;
        transition: all 1s ease-in-out;
        @media only screen and (max-width: 750px) {
          padding: 30px 0;
          @include flex(1);
          text-align: center !important;
          @include align-self(flex-end !important);
          -webkit-transition: all 1.5s ease-in-out;
          -moz-transition: all 1.5s ease-in-out;
          -ms-transition: all 1.5s ease-in-out;
          -o-transition: all 1.5s ease-in-out;
          transition: all 1.5s ease-in-out;
        }

        &.content_middle-aligned {
          @include align-self(center);
        }

        &.content_bottom-aligned {
          @include align-self(flex-end);
        }

        &.full-width-alignment1 {
          @include flex(0 0 100%);
          @include flexbox();
          @include flex-direction(row);
          @include align-items(center);
          @media only screen and (max-width: 768px) {
            @include flex-direction(column);
            @include justify-content(center);
            @include align-self(flex-end);
          }

          h1 {
            max-width: 640px;
            @media only screen and (max-width: 768px) {
              text-align: center;
            }
          }

          .btn_style1 {
            margin-left: auto;
            @media only screen and (max-width: 768px) {
              margin-left: 0;
            }
          }
        }

        &.full-width-alignment2 {
          @include flex(0 0 100%);
          @include flexbox();
          @include flex-direction(row-reverse);
          @include align-items(center);
          @media only screen and (max-width: 768px) {
            @include flex-direction(column);
            @include justify-content(center);
            @include align-self(flex-end);
          }

          h1 {
            max-width: 640px;
            text-align: right;
            @media only screen and (max-width: 768px) {
              text-align: center;
            }
          }

          .btn_style1 {
            margin-right: auto;
            @media only screen and (max-width: 768px) {
              margin-right: 0;
            }
          }
        }

        h1 {
          font-size: 45px;
          font-weight: 700;
          letter-spacing: 6px;
          color: #FFFFFF;
          @media only screen and (max-width: 768px) {
            font-size: 24px;
          }
        }

        h5 {
          font-size: 18px;
          text-transform: uppercase;
          font-family: "Source Sans Pro",sans-serif;
          font-weight: 400;
          letter-spacing: 3.5px;
          color: #FFFFFF;
          @media only screen and (max-width: 768px) {
            font-size: 14px;
            font-weight: 600;
          }
        }

        p {
          font-size: 16px;
          line-height: 18px;
          color: #FFFFFF;
          @media only screen and (max-width: 768px) {
            font-size: 14px;
          }
        }
      }
    }
  }

  .slick-current {
    .sw__banner-img-cont {
      left: 16% !important;
      opacity: 1 !important;
      @media only screen and (max-width: 768px) {
        left: 0 !important;
      }
    }
    .sw__banner-text-container {
      top: 0 !important;
      opacity: 1 !important;
    }
  }

  .slick-dots {
    text-align: right;
    position: absolute;
    right: 0;
    bottom: 10px;
    display: inline-block !important;
    li {
      padding: 10px 20px;
      margin: 0px;
      width: auto;
      height: auto;
      display: list-item;
      text-align: right;
      transition: 300ms all;

      button, a {
        padding: 10px 30px 10px 0;
        width: auto;
        height: auto;
        font-size: 14px;
        font-weight: 600;
        letter-spacing: 1.4px;
        line-height: 1;
        text-transform: uppercase;
        color: #FFFFFF;
        display: inline-block;
        border-right: 4px solid #FFF;
        outline: none;
        opacity: 0.6;

        &:before {
          display: none;
        }
      }
    }

    .slick-slide {
      &.slick-current {
        button, a {
          opacity: 1;
        }
      }
    }
  }
}

