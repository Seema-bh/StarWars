              @include flex(0 0 90px);
            }
          }	
          .cart__content--desc {	
            padding: 15px;	
            @include flex(1);
            .cart_item-sku {	
              font-size: 11px;	
              text-transform: uppercase;	
              letter-spacing: 2px;	
              color: #305C8C;	
              display: block;	
            }	
            a.cart__product-title {
              margin-bottom: 3px;
              max-width: 350px;	
              width: 100%;
              font-family: "Playfair Display",serif;
              font-weight: 500;
              font-size: 18px;
              line-height: 1.2;
              color: #242424;	
              display: block;	
              text-decoration: none;	
              outline: none;	
              border: none;	
            }	
            .cart__content--property {	
              margin-top: 10px;	
              @include flexbox();	
              @include flex-wrap(wrap);	
              .cart__content--size {	
                padding-right: 2%;	
                @include flex(1);

                @media (max-width: 768px) {	
                  -webkit-box-ordinal-group: 3;	
                  -ms-flex-order: 2;	
                  order: 2;	
                }	
                .product-details {	
                  padding: 0;	
                  margin: 0;	
                  font-size: 14px;
                  letter-spacing: 2px;
                  text-transform: uppercase;
                  font-weight: 600;
                  .product-details__item {	
                    margin-bottom: 0;	
                    font-size: 14px;
                    font-weight: 600;
                    letter-spacing: 2px;	
                    text-transform: uppercase;	

                    .cart_item-size {	
                      margin-top: 0;	
                      font-size: 14px;
                      font-weight: 600;
                      color: #242424;	
                      display: inline-block;	
                    }	
                  }	
                }	
              }	
              .cart__content--quantity {	
                @include flex(1);
                text-align: center;

                @media (max-width: 768px) {	
                  -webkit-box-ordinal-group: 4;	
                  -ms-flex-order: 3;	
                  order: 3;	
                }	

                .cart__qty {
                  margin: 0;
                  background: #FFF;
                  border: 1px solid rgba(94, 114, 125, 0.26);
                  display: inline-flex;
                  @media (max-width: 768px) {	
                    @include flexbox();
                  }
                }	
                .cart__qty-label {	
                  margin-bottom: 10px;	
                  width: 100%;	
                  height: auto;	
                  position: static !important;	
                  font-size: 10px;
                  font-weight: 600;
                  line-height: 16px;	
                  letter-spacing: 2px;	
                  text-transform: uppercase;	
                  color: #5E727D;	
                }	
                .qty_minus,	
                .qty_plus {	
                  padding: 0;
                  background: none;
                  width: 40px;	
                  height:30px;
                  outline: none;	
                  border: none;
                  svg {
                    vertical-align: text-bottom;
                  }
                }	
                .cart__qty-input {	
                  padding: 0;	
                  margin: 0;
                  outline: none;	
                  border: none;	
                  font-size: 16px;	
                  font-weight: 600;
                  color: #242424;	
                  @include flex(1);
                  pointer-events: none;
                  width: 40px;
                  &::-webkit-inner-spin-button,	
                  &::-webkit-outer-spin-button {	
                    -webkit-appearance: none; 	
                    margin: 0;	
                  }	
                }	
              }	
            }	
          }	
          .cart__content--btns {	
            padding: 15px 0;	
            @include flexbox();	
            @include flex-direction(column);	
            @include justify-content(flex-end);
            @media (max-width: 768px) {	
              padding: 0 1em 15px 1em;
              width: 100%;
            }
            .cart__content--price {	
              margin-top: auto;
              @media (max-width: 768px) {	
                margin-bottom: 10px;	
                width: 100%;	
