      }
      .after-touch{
        display:none;
      }
    }
    .wishlish-button-wrapper{
      display: flex;
      justify-content: space-between;
      @media (max-width:767px){
        flex-direction: column;
      }
      .btn{
        letter-spacing: 3px;
        @media (max-width:767px){
          min-width: auto;
        }
      }
    }
    .add-to-cart-wrapper{
