/* Custom CSS for Wiser */    


/***** LOGIN PAGE AND SIGN UP PAGE *****/
.login__container,
.register__container {
  min-height: 500px;
  @include flexbox();
  @include align-items(center);
  margin-bottom: 55px;

  @media(max-width:749px){
    min-height: auto;
    @include align-items(flex-start);
    margin-bottom: 35px;
  }
}

.login__form,
.register__form,
.recover__password {
  width: 100%;
  padding: 20px 35px;
  margin: 0 auto;
  max-width: 450px; 
  text-align: center;
  -webkit-box-shadow: 0px 0px 10px 3px rgba(0,0,0,0.04);
  -moz-box-shadow: 0px 0px 10px 3px rgba(0,0,0,0.04);
  box-shadow: 0px 0px 10px 3px rgba(0,0,0,0.04);

  @media(max-width:749px){
    padding: 20px 0;
    box-shadow: none;
  }
  .input--error .dropdown {
    border-color: #c61c1c !important; 
  }
  .birthdate {
    position: relative;
    width: 100%;
    
  }
  .birthdate label {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 3px;
    background-color: #fff;
    padding: 6px 30px 6px 10px;
    margin: 0;
    color: #5E727D;
    opacity: 1;
  }
  
  .birthdate input[type=date]:invalid + label {
    opacity: 1;
  }
  
  .birthdate input[type=date]:valid + label,
  .birthdate input[type=date]:focus + label{
    opacity: 0;
  }

  .login__heading,
  .register__heading {
    color: #242424;
    font-size: 32px;
    line-height: 43px;
    font-weight: 700;
    letter-spacing: 0;
    text-transform: capitalize;
    margin-bottom: 5px;
  }

  .recover__heading {
    margin: 0;
    color: #242424;
    font-size: 20px;
    font-weight: 700;    
    letter-spacing: 0;
  }

  .form__errors,
  .login__subheading,
  .register__subheading,
  .recover__subheading {
    color: #5E727D;
    font-size: 14px;
    line-height: 18px;
    margin-bottom: 25px;
  }

  [data-form-error] {
    text-align: left;
    color: #C61C1C;
  }

  .form__errors,
  .form__errors li {
    color: #C61C1C;
  }

  .input__group {
    margin: 0 0 20px;

    input {
      width: 100%;
      min-height: 40px;
      padding: 8px 10px;
      border-color: #ced6da;
      font-size: 16px;
      line-height: 18px; 
      border-radius: 0;
      background: #fff;
      margin-bottom: 0 !important;
      &.input--error {
        border-color: #C61C1C;
      }
      &::placeholder {
        color: #5E727D;
        opacity: 1;
      }
    }
  }

  .form__actions {
    @include flexbox();
    @include align-items(center);
    @include justify-content(space-between);

    .action__item {
      @include flexbox();
      @include align-items(center);

      label {
        color: #5E727D;
        margin-left: 4px;
      }

      .input-checkbox {
        position: relative;
        input[type="checkbox"] {
          position: absolute;
          top: 0;
          left: 0;
          opacity: 0;
          &:checked + label {
            &:before {background: #ced6da;}
            &:after {opacity: 1;}
          }
        }
        label {
          position: relative;
          padding-left: 16px;
          margin: 0;
          &:before {
            content: '';
            display: block;
            position: absolute;
            left: 0;
            top: 6px;
            border: 1px solid #ced6da;
            width: 12px;
            height: 12px;
          }
          &:after {
            content: '';
            display: block;
            position: absolute;
            top: 7px;
            left: 4px;
            width: 4px;
            height: 8px;
            border: solid #fff;
            border-width: 0 1px 1px 0;
            transform: rotate(45deg);
            opacity: 0;
          }
        }
      }
      #RecoverPassword {
        font-family: 'Barlow', sans-serif;
        color: #3D4A51;
        font-size: 12px;
        font-weight: 700;
        text-transform: uppercase;
      }
    }
  }

  .form__submission {
    margin: 20px 0 15px;

    .btn {
      background: #242424;
      width: 100%;
      padding: 8px 10px;
      font-family: 'Barlow', sans-serif;
      font-size: 16px;
      line-height: 18px;
      font-weight: 600;
      letter-spacing: 2.4px;
      display: flex;
      justify-content: center;
      align-items: center;
      svg {margin: 0 0 0 10px;}
    }
  }

  .form__message {
    p {
      color: #5E727D;
      font-size: 14px;
      line-height: 18px;
    }
    a {
      color: #f4513b;
      font-weight: 600;
      text-decoration: underline;
    }
  }
}

/* Custom select dropdown */
.register__form {
  .select-container {
    select {display: none;}
    .dropdown {
      border: solid 1px #ced6da;
      cursor: pointer;
      display: block;
      font-size: 16px;
      font-weight: 400;
      height: 40px;
      line-height: 38px;
      outline: 0;
      padding-left: 10px;
      padding-right: 28px;
      position: relative;
      text-align: left !important;
      transition: all .2s ease-in-out;
      user-select: none;
      white-space: nowrap;
      width: 100%;
      border-radius: 0;
      color:#5E727D;
      &:active {
        border-color: #e6e6e6;
        box-shadow: 0 1px 4px rgba(0,0,0,.05) inset;
      }
      // Arrow
      &:after { 
        height: 0;
        width: 0;
        border-left: 4px solid transparent;
        border-right: 4px solid transparent;
        border-top: 4px solid #777;
        transform: origin(50% 20%);
        transition: all 0.125s ease-in-out;
        content: '';
        display: block; 
        margin-top: -2px;
        pointer-events: none;
        position: absolute; 
        right: 10px; top: 50%; 
      }
      &.open {
        &:after {transform: rotate(-180deg);}
        .list {
          transform: scale(1);
          opacity: 1;
          pointer-events: auto;
        }
        .option {cursor: pointer;}
      }
      .list {
/*         transition: all .15s cubic-bezier(0.25, 0, 0.25, 1.75), opacity .1s linear; */
        transform: scale(.75);
        transform-origin: 50% 0;
        box-shadow: 0 0 0 1px rgba(0, 0, 0, .09);
        background-color: #fff;
        width: 100%;
        border-radius: 0;
        margin-top: 1px;
        padding: 3px 0;
        opacity: 0;
        overflow: hidden;
        pointer-events: none;
        position: absolute;
        top: 100%; left: 0;
        z-index: 999;
        &:hover .option:not(:hover) {background-color: transparent !important;}
      }
      .option {
        cursor: default;
        font-weight: 400;
        outline: none;
        padding-left: 10px;
        padding-right: 28px;
        text-align: left;
        transition: all 0.2s;
        &:hover, &:focus { background-color: #ebeef0;}
        &.selected { font-weight: 600;}
        &.selected:focus { background: #ebeef0;}
        &:first-child{display:none;}
      }
    }
  }
}

