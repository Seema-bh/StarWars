  }
}

.sw__header {
  padding-top: 15px;
  padding-bottom: 15px;
  @media only screen and (max-width: 750px) {
    padding-top: 0 !important;
    padding-bottom: 0;
  }

  .sw__header-nav {
    li {
      display: inline;

      &:after {
        width: 3px;
        height: 3px;
        border-radius: 50%;
        background: #242424;
        content: "";
        display: inline-block;
        vertical-align: 2px;
        margin: 0 10px;
      }

      &:last-child {
        &:after {
          display: none;
        }
      }

      a {
        font-size: 10px;
        font-weight: bold;
        text-transform: uppercase;
        letter-spacing: 1.5px;
        color: #242424;
      }
    }
  }

  .site-header__icons-wrapper {
    .site-header__icon {
      @include flexbox();
      @include align-items(center);

      svg {
        @media (max-width: 750px) {
          width: 30px;
          height: 30px;
        }
      }

      span {
        margin-left: 5px;
        font-size: 14px;
        color: #242424;
        @media(max-width: 1024px) {
          margin-left: 0;
          font-size: 16px;
        }
      }
    }
  }
}

.sw__nav {
  @include flexbox();
  @include justify-content(center);
  background: #EBEEF0;
  position: relative;
  > ul {
    margin: 0;
    padding: 0 0;

    li {
      a,button .site-nav__label {
        font-size: 14px;
        letter-spacing: 1px;
        line-height: 18px;
        font-weight: 600;
        text-transform: uppercase;
        color: #5E727D;
        padding:10px 25px !important;
        .site-nav__label {
          border-bottom: 4px solid transparent;
          @include transition();
        }

        &:hover {
          .site-nav__label {
            border-bottom: 4px solid #F4513B !important;
          }
        }
      }
      &[data-has-dropdowns]:hover .site-nav__dropdown {
        display: block;
        a{
          color: #242424;
          padding:5px 20px !important
        }
        .site-nav__child-link span {
          text-transform: capitalize;
          font-size:16px;
          font-weight: 400;
        }
        .site-nav__child-link--parent{
          font-size:14px;
          padding-top: 0 !important;
          margin: 0 !important;
          span{
            text-transform: uppercase;
            font-weight: 700;
            letter-spacing: 2.5px;
          }
        }
      }

    }
    .site-nav__link--main {
      .site-nav--active {
        a {
          font-weight: 700;
          color: #242424;

          .site-nav__label {
            border-bottom: 4px solid #F4513B;
          }
        }
      }
    }
  }

  .site-header__search-toggle {
    @include flexbox();
    @include align-items(center);
    outline: none;
    padding: 0 25px;
    span {
      font-size: 14px;
      letter-spacing: 1.4px;
      line-height: 18px;
      font-weight: 600;
      text-transform: uppercase;
      color: #5E727D;
      display:none;
    }
  }
}

.search-form__input-wrapper {
  input {
    outline: none;
  }
}

[data-section-type="starwars-banner-section"] {
