<style>
  .popup-wrapper{
    position: relative;
  }
  .popup-wrapper ul{
    padding-left:20px;
  }
  .popup-wrapper ul li{
    list-style: disc;
  }
  .popup-wrapper .site-header__logo {
    padding-left: 0;
  }
  .popup-wrapper .site-header__logo img {
    margin: 0 auto;
  }
  .vc-overlay {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(0, 0, 0, 0.7);
    transition: opacity 500ms;
    z-index:999;
    visibility: hidden;
    opacity: 0;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  .close-wrapper{
    text-align:center;
  }
  .popup {
    margin: 0 auto;
    padding: 30px;
    background: #fff;
    border-radius: 5px;
    max-width: 700px;
    max-height: 80vh;
    position: relative;
    transition: all 1s ease-in-out;
    overflow: auto;
  }
  .popup p{
    margin:0;
    line-height:1.3;
  }
  .popup-wrapper .close {
    transition: all 200ms;
    font-size: 18px;
    font-weight: bold;
    text-decoration: none;
    color: #fff;
    background: #000;
    padding: 5px 20px;
    border-radius: 0;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    
  }
  .popup-wrapper .close:hover {
    color:#fff;
  }
  @media (max-width:767px){
    .popup {
      width: 95%;
      padding: 10px;
    }
    .popup p{
      margin:0;
    }
  }
  
