    </script>
    {% elsif template contains 'cart' %}
    <script type="text/javascript">
      var id = new Array();
      {% for item in cart.items %}
      id.push('shopify_JP_{{item.product.id}}_{{item.variant.id}}');
              {% endfor %}

              var google_tag_params = {
